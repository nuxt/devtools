{
  "private": false,
  "packageManager": "pnpm@7.27.1",
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm -C packages/devtools dev",
    "lint": "eslint .",
    "typecheck": "nuxi typecheck",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "pnpm test && bumpp -x \"pnpm run changelog\" --all && pnpm -r publish",
    "test": "pnpm lint"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@antfu/ni": "^0.20.0",
    "@nuxt/devtools": "workspace:*",
    "@nuxt/module-builder": "^0.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.14.1",
    "@types/pacote": "^11.1.5",
    "@types/wait-on": "^5.3.1",
    "@types/which": "^2.0.2",
    "@types/ws": "^8.5.4",
    "@unocss/eslint-config": "^0.50.0",
    "bumpp": "^8.2.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.34.0",
    "nuxt": "^3.2.2",
    "typescript": "^4.9.5",
    "vue-tsc": "^1.1.7"
  }
}
