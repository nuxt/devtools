{
  "name": "@nuxt-devtools/monorepo",
  "type": "module",
  "version": "0.8.3",
  "private": true,
  "packageManager": "pnpm@8.7.4",
  "scripts": {
    "build": "pnpm -r --filter=\"./packages/**/*\" run build",
    "stub": "pnpm -r run stub",
    "dev": "pnpm run stub && pnpm -C packages/devtools dev",
    "lint": "eslint .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "pnpm test && bumpp -r -x \"pnpm run changelog\" --all && pnpm -r publish",
    "test": "pnpm lint",
    "docs": "nuxi dev docs",
    "typecheck": "vue-tsc --noEmit",
    "postinstall": "npx simple-git-hooks",
    "prepare": "pnpm -r --filter=\"./packages/*\" run dev:prepare"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@antfu/ni": "^0.21.8",
    "@nuxt/devtools": "workspace:*",
    "@nuxt/devtools-ui-kit": "workspace:*",
    "@nuxt/module-builder": "^0.5.1",
    "@nuxt/schema": "^3.7.1",
    "@types/markdown-it": "^13.0.1",
    "@types/node": "^20.5.9",
    "@types/pacote": "^11.1.5",
    "@types/wait-on": "^5.3.1",
    "@types/which": "^3.0.0",
    "@types/ws": "^8.5.5",
    "@unocss/eslint-config": "^0.55.7",
    "bumpp": "^9.2.0",
    "conventional-changelog-cli": "^4.0.0",
    "eslint": "8.48.0",
    "esno": "^0.17.0",
    "execa": "^7.2.0",
    "gray-matter": "^4.0.3",
    "lint-staged": "^14.0.1",
    "nuxt": "^3.7.1",
    "pathe": "^1.1.1",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.2",
    "tiged": "^2.12.5",
    "typescript": "^5.2.2",
    "ua-parser-js": "^1.0.35",
    "unocss": "^0.55.7",
    "vite-hot-client": "^0.2.2",
    "vue-tsc": "^1.8.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
