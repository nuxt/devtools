{
  "name": "@nuxt/devtools",
  "version": "0.0.0",
  "repository": "nuxt/devtools",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "import": "./dist/module.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "workspaces": [
    "client"
  ],
  "scripts": {
    "build": "pnpm dev:prepare && pnpm build:module && pnpm build:client",
    "build:client": "nuxi generate client",
    "build:module": "nuxt-build-module",
    "dev": "nuxi dev client",
    "dev:playground": "pnpm build && nuxi dev playground",
    "dev:prepare": "nuxi prepare client",
    "lint": "eslint --ext .ts,.vue,.mjs .",
    "prepublishOnly": "pnpm build",
    "typecheck": "nuxi typecheck",
    "release": "pnpm test && standard-version && git push --follow-tags && npm publish",
    "test": "pnpm lint"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.2",
    "@nuxt/kit": "^3.0.0",
    "birpc": "^0.2.3",
    "execa": "^6.1.0",
    "flatted": "^3.2.7",
    "get-port-please": "^3.0.1",
    "h3": "^1.0.2",
    "hookable": "^5.4.2",
    "launch-editor": "^2.6.0",
    "markdown-it": "^13.0.1",
    "pathe": "^1.1.0",
    "sirv": "^2.0.2",
    "tinyws": "^0.1.0",
    "unimport": "^1.3.0",
    "vite-plugin-inspect": "^0.7.14",
    "vite-plugin-vue-inspector": "^3.2.1",
    "wait-on": "^7.0.1",
    "which": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.1",
    "@antfu/ni": "^0.19.0",
    "@nuxt/devtools": "workspace:*",
    "@nuxt/module-builder": "^0.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.11.18",
    "@types/wait-on": "^5.3.1",
    "@types/which": "^2.0.1",
    "@types/ws": "^8.5.4",
    "@unocss/nuxt": "^0.48.4",
    "bumpp": "^8.2.1",
    "eslint": "^8.32.0",
    "floating-vue": "2.0.0-beta.20",
    "json-editor-vue": "^0.10.5",
    "nuxt": "npm:nuxt3@3.0.1-rc.0-27909319.bb41265",
    "typescript": "^4.9.4",
    "vanilla-jsoneditor": "^0.14.0",
    "vue-tsc": "^1.0.24",
    "ws": "^8.12.0"
  },
  "packageManager": "pnpm@7.25.1",
  "pnpm": {
    "overrides": {
      "nuxt": "npm:nuxt3@latest",
      "nitropack": "npm:nitropack-edge@latest"
    }
  }
}
