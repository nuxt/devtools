{
  "name": "@nuxt-devtools/monorepo",
  "type": "module",
  "version": "1.0.8",
  "private": true,
  "packageManager": "pnpm@8.15.5",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt/devtools.git"
  },
  "scripts": {
    "build": "pnpm -r --filter=\"./packages/**/*\" run build",
    "stub": "pnpm -r run stub",
    "dev": "pnpm run stub && pnpm -C packages/devtools dev",
    "lint": "eslint .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "pnpm test && bumpp -r -x \"pnpm run changelog\" --all && pnpm -r publish",
    "test": "pnpm lint",
    "docs": "nuxi dev docs",
    "docs:build": "CI=true nuxi generate docs",
    "typecheck": "vue-tsc --noEmit",
    "postinstall": "npx simple-git-hooks && pnpm -C docs install",
    "prepare": "pnpm -r --filter=\"./packages/*\" run dev:prepare"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.3",
    "@antfu/ni": "^0.21.12",
    "@nuxt/devtools": "workspace:*",
    "@nuxt/devtools-ui-kit": "workspace:*",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.11.1",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^20.11.30",
    "@types/pacote": "^11.1.8",
    "@types/which": "^3.0.3",
    "@types/ws": "^8.5.10",
    "@unocss/eslint-config": "^0.58.6",
    "bumpp": "^9.4.0",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "esno": "^4.7.0",
    "execa": "^7.2.0",
    "gray-matter": "^4.0.3",
    "lint-staged": "^15.2.2",
    "nuxt": "^3.11.1",
    "pathe": "^1.1.2",
    "simple-git-hooks": "^2.11.0",
    "taze": "^0.13.3",
    "tiged": "^2.12.7",
    "typescript": "^5.4.2",
    "ua-parser-js": "^1.0.37",
    "unocss": "^0.58.6",
    "vite-hot-client": "^0.2.3",
    "vue-tsc": "^2.0.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
