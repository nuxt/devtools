{
  "name": "@nuxt/devtools-ui-kit",
  "type": "module",
  "version": "1.0.2",
  "license": "MIT",
  "homepage": "https://devtools.nuxt.com/module/ui-kit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt/devtools.git",
    "directory": "packages/devtools-ui-kit"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./module.cjs"
    },
    "./module": {
      "import": "./dist/module.mjs",
      "require": "./module.cjs"
    },
    "./unocss": {
      "import": "./dist/unocss.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "*.cjs",
    "dist"
  ],
  "scripts": {
    "build": "nuxt-build-module build",
    "stub": "nuxt-build-module build --stub",
    "dev": "nuxi dev playground",
    "prepublishOnly": "npm run build",
    "playground:build": "nuxi generate playground"
  },
  "peerDependencies": {
    "@nuxt/devtools": "workspace:*"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.1.21",
    "@iconify-json/logos": "^1.1.38",
    "@iconify-json/ri": "^1.1.12",
    "@iconify-json/tabler": "^1.1.97",
    "@nuxt/devtools-kit": "workspace:*",
    "@nuxt/kit": "^3.8.1",
    "@nuxtjs/color-mode": "^3.3.2",
    "@unocss/core": "^0.57.6",
    "@unocss/nuxt": "^0.57.6",
    "@unocss/preset-attributify": "^0.57.6",
    "@unocss/preset-icons": "^0.57.6",
    "@unocss/preset-mini": "^0.57.6",
    "@unocss/reset": "^0.57.6",
    "@vueuse/core": "^10.6.1",
    "@vueuse/integrations": "^10.6.1",
    "@vueuse/nuxt": "^10.6.1",
    "defu": "^6.1.3",
    "focus-trap": "^7.5.4",
    "splitpanes": "^3.1.5",
    "unocss": "^0.57.6",
    "v-lazy-show": "^0.2.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "workspace:*",
    "nuxt": "^3.8.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
