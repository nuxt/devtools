{
  "name": "@nuxt/devtools-kit",
  "type": "module",
  "version": "1.0.0-beta.0",
  "license": "MIT",
  "repository": "nuxt/devtools",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.cjs"
    },
    "./iframe-client": {
      "types": "./iframe-client.d.ts",
      "import": "./iframe-client.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "*.cjs",
    "*.d.ts",
    "*.mjs",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "dev:prepare": "nr stub",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "nuxt": "^3.7.4",
    "vite": "*"
  },
  "dependencies": {
    "@nuxt/kit": "^3.7.4",
    "@nuxt/schema": "^3.7.4",
    "execa": "^7.2.0"
  },
  "devDependencies": {
    "birpc": "^0.2.14",
    "error-stack-parser-es": "^0.1.1",
    "hookable": "^5.5.3",
    "unbuild": "^2.0.0",
    "unimport": "^3.4.0",
    "vite-plugin-vue-inspector": "^3.7.2",
    "vue-router": "^4.2.5"
  }
}
