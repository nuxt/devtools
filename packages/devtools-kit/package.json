{
  "name": "@nuxt/devtools-kit",
  "type": "module",
  "version": "0.6.6",
  "license": "MIT",
  "repository": "nuxt/devtools",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./types": {
      "types": "./types.d.ts",
      "require": "./dist/types.cjs",
      "import": "./dist/types.mjs"
    },
    "./iframe-client": {
      "types": "./iframe-client.d.ts",
      "import": "./iframe-client.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts",
    "*.cjs",
    "*.mjs"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "dev:prepare": "nr stub",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "nuxt": "^3.5.1",
    "vite": "*"
  },
  "dependencies": {
    "@nuxt/kit": "^3.6.1",
    "@nuxt/schema": "^3.6.1",
    "execa": "^7.1.1"
  },
  "devDependencies": {
    "birpc": "^0.2.12",
    "hookable": "^5.5.3",
    "unbuild": "^1.2.1",
    "unimport": "^3.0.12",
    "vite-plugin-vue-inspector": "^3.4.2",
    "vue-router": "^4.2.2"
  }
}
