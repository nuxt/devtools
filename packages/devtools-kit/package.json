{
  "name": "@nuxt/devtools-kit",
  "type": "module",
  "version": "2.0.0-beta.7",
  "license": "MIT",
  "homepage": "https://devtools.nuxt.com/module/utils-kit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt/devtools.git",
    "directory": "packages/devtools-kit"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.cjs"
    },
    "./iframe-client": {
      "types": "./iframe-client.d.ts",
      "import": "./iframe-client.mjs"
    },
    "./host-client": {
      "types": "./host-client.d.ts",
      "import": "./host-client.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "*.cjs",
    "*.d.ts",
    "*.mjs",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "dev:prepare": "nr stub",
    "prepack": "pnpm build"
  },
  "peerDependencies": {
    "vite": ">=6.0"
  },
  "dependencies": {
    "@nuxt/kit": "catalog:",
    "@nuxt/schema": "catalog:",
    "execa": "catalog:"
  },
  "devDependencies": {
    "birpc": "catalog:",
    "error-stack-parser-es": "catalog:",
    "hookable": "catalog:",
    "unbuild": "catalog:",
    "unimport": "catalog:",
    "vite-plugin-vue-inspector": "catalog:",
    "vue-router": "catalog:"
  }
}
