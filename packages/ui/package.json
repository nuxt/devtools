{
  "name": "@nuxt/ui",
  "version": "0.4.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/nuxt.mjs",
      "require": "./nuxt.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.mjs",
      "require": "./nuxt.cjs"
    },
    "./unocss": {
      "import": "./dist/unocss.mjs"
    }
  },
  "main": "./dist/nuxt.mjs",
  "files": [
    "dist",
    "*.cjs"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nuxi dev playground",
    "lint": "eslint . --ext .vue,.ts",
    "prepack": "npm run build",
    "playground:build": "nuxi build playground"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.1.13",
    "@nuxt/kit": "^3.0.0",
    "@nuxtjs/color-mode": "^3.2.0",
    "@unocss/core": "^0.48.2",
    "@unocss/nuxt": "^0.48.2",
    "@unocss/preset-attributify": "^0.48.2",
    "@unocss/preset-icons": "^0.48.2",
    "@unocss/preset-mini": "^0.48.2",
    "@unocss/reset": "^0.48.2",
    "@vueuse/core": "^9.10.0",
    "@vueuse/integrations": "^9.10.0",
    "@vueuse/nuxt": "^9.10.0",
    "defu": "^6.1.1",
    "focus-trap": "^7.2.0",
    "unocss": "^0.48.2"
  },
  "devDependencies": {
    "nuxt": "^3.0.0",
    "unbuild": "^1.0.2"
