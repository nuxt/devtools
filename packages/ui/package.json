    "@unocss/core": "^0.31.2",
    "@unocss/nuxt": "^0.31.2",
    "@unocss/preset-attributify": "^0.31.2",
    "@unocss/preset-icons": "^0.31.2",
    "@unocss/preset-mini": "^0.31.2",
    "@unocss/reset": "^0.31.2",
    "@vueuse/core": "^8.2.5",
    "@vueuse/integrations": "^8.2.5",
    "@vueuse/nuxt": "^8.2.5",
    "defu": "^6.0.0",
    "focus-trap": "^6.7.3",
    "unocss": "^0.31.2"
