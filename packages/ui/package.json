{
  "name": "@nuxt/ui",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/nuxt.mjs",
      "require": "./nuxt.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.mjs",
      "require": "./nuxt.cjs"
    }
  },
  "main": "./dist/nuxt.mjs",
  "files": [
    "dist",
    "*.cjs"
