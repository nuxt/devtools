{
  "name": "@nuxt/ui",
  "version": "0.3.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/nuxt.mjs",
      "require": "./nuxt.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.mjs",
      "require": "./nuxt.cjs"
    },
    "./unocss": {
      "import": "./dist/unocss.mjs"
    }
  },
  "main": "./dist/nuxt.mjs",
  "files": [
    "dist",
    "*.cjs"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nuxi dev playground",
    "lint": "eslint . --ext .vue,.ts",
    "prepack": "npm run build",
    "playground:build": "nuxi build playground"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.1.7",
    "@nuxt/kit": "^3.0.0-rc.6",
    "@nuxtjs/color-mode": "^3.1.4",
    "@unocss/core": "^0.45.5",
    "@unocss/nuxt": "^0.45.5",
    "@unocss/preset-attributify": "^0.45.5",
    "@unocss/preset-icons": "^0.45.5",
    "@unocss/preset-mini": "^0.45.5",
    "@unocss/reset": "^0.45.5",
    "@vueuse/core": "^9.1.0",
    "@vueuse/integrations": "^9.1.0",
    "@vueuse/nuxt": "^9.1.0",
    "defu": "^6.0.0",
    "focus-trap": "^6.9.4",
    "unocss": "^0.45.5"
  },
  "devDependencies": {
    "nuxt": "npm:nuxt3@latest",
    "unbuild": "^0.7.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
